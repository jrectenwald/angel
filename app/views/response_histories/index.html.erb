<p id="notice"><%= notice %></p>

<h1>Response Histories</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Question</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @response_histories.each do |response_history| %>
      <tr>
        <td><%= response_history.user.name %></td>
        <td><%= response_history.question.query %></td>
        <td><%= link_to 'Show', response_history %></td>
        <td><%= link_to 'Edit', edit_response_history_path(response_history) %></td>
        <td><%= link_to 'Destroy', response_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Response History', new_response_history_path %>
